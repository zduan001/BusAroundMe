<UserControl
    x:Class="BusAroundMe.BusStopIcon"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="BusAroundMe:MyBus"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400">
    
    <Grid>
        <Canvas>
            <Rectangle Height="25" Width="25" Fill="#FFF0F0F0" Margin="-12.5,-12.5,0,0"></Rectangle>
            <Rectangle Height="20" Width="20" Fill="Black" RadiusX="4" RadiusY="4" Margin="-10,-10,0,0"></Rectangle>
            <Rectangle Height="9" Width="16" Fill="Yellow" Canvas.Left="-8" Canvas.Top="-7"/>
            <Rectangle Height="12" Width="5" Fill="Black" RadiusX="4" RadiusY="4" Canvas.Left="-10" Canvas.Top="3"/>
            <Rectangle Height="12" Width="5" Fill="Black" RadiusX="4" RadiusY="4" Canvas.Left="5" Canvas.Top="3"/>
            <Ellipse Height="3" Width="3" Fill="Yellow" Canvas.Left="-8" Canvas.Top="4"/>
            <Ellipse Height="3" Width="3" Fill="Yellow" Canvas.Left="4" Canvas.Top="4"/>
        </Canvas>
    </Grid>
</UserControl>
